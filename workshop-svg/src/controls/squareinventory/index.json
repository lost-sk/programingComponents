      {
        "label": "微软雅黑",
        "value": "微软雅黑"
      },
      {
        "label": "黑体",
        "value": "黑体"
      },
      {
        "label": "楷体",
        "value": "楷体"
      },
      {
        "label": "隶书",
        "value": "隶书"
      },
      {
        "label": "幼圆",
        "value": "幼圆"
      },
      {
        "label": "宋体",
        "value": "宋体"
      }
