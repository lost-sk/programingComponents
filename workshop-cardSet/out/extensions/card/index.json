    "placeholder": "14px",
    "inputType": "Input"
  },
  {
    "name": "fontFamily",
    "displayName": "标题字体",
    "resource": "select.js",
    "options": [
      {
        "label": "微软雅黑",
        "value": "微软雅黑"
      },
      {
        "label": "黑体",
        "value": "黑体"
      },
      {
        "label": "楷体",
        "value": "楷体"
      },
      {
        "label": "隶书",
        "value": "隶书"
      },
      {
        "label": "幼圆",
        "value": "幼圆"
      },
      {
        "label": "宋体",
        "value": "宋体"
      }
    ],
    "defaultValue": "微软雅黑"
  },
  {
    "name": "fontBorder",
    "displayName": "字体加粗",
    "resource": "select.js",
    "options": [
      {
        "label": "默认",
        "value": "normal"
      },
      {
        "label": "加粗",
        "value": "bolder"
      }
    ],
    "defaultValue": "normal"
  },
  {
    "name": "fontColor",
    "displayName": "标题颜色",
    "restore": false,
    "inputType": "ColorPicker"
