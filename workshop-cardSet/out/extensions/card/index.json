    "options": [
      {
        "label": "显示",
        "value": "true"
      },
      {
        "label": "隐藏",
        "value": "false"
      }
    ],
    "defaultValue": "true"
  },
  {
    "name": "borderColor",
    "displayName": "边框颜色",
    "inputType": "ColorPicker"
  },
  {
    "name": "object",
    "displayName": "数据源",
    "inputType": "ObjectInput",
    "showCopyIcon": false
  },
  {
    "name": "htmlDetail",
    "resource": "html.js"
  },
  {
    "name": "events",
    "resource": "events.js"
